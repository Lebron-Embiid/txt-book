RewriteEngine On

RewriteRule ^index.html$                               index.php [QSA,L]
RewriteRule ^index-(.*).html$                          index.php?a=$1 [QSA,L]

RewriteRule ^case.html$                                case.php [QSA,L]
RewriteRule ^case_detail.html$                         case_detail.php [QSA,L]
RewriteRule ^case-detail-(.*).html$                    case_detail.php?a=$1 [QSA,L]
RewriteRule ^case-(.*)-(.*).html$                      case.php?a=$1&page=$2 [QSA,L]
RewriteRule ^case-(.*).html$                           case.php?a=$1 [QSA,L]

RewriteRule ^solution-detail-(.*).html$                solution_detail.php?a=$1 [QSA,L]
RewriteRule ^solution.html$                            solution.php [QSA,L]

RewriteRule ^news.html$                                news.php [QSA,L]
RewriteRule ^news-detail.html$                         news_detail.php [QSA,L]
RewriteRule ^news-detail-(.*).html$                    news_detail.php?a=$1 [QSA,L]
RewriteRule ^news-(.*)-(.*).html$                      news.php?a=$1&page=$2 [QSA,L]
RewriteRule ^news-(.*).html$                           news.php?a=$1 [QSA,L]

RewriteRule ^template.html$                            template.php [QSA,L]
RewriteRule ^template_detail.html$                     template_detail.php [QSA,L]
RewriteRule ^template-detail-(.*).html$                template_detail.php?a=$1 [QSA,L]
RewriteRule ^template-detail-(.*)-(.*).html$           template_detail.php?a=$1&page=$2 [QSA,L]

RewriteRule ^website.html$                             website.php [QSA,L]
RewriteRule ^applets.html$                             applets.php [QSA,L]
RewriteRule ^system.html$                              system.php [QSA,L]
RewriteRule ^about_us.html$                            about_us.php [QSA,L]
RewriteRule ^contact.html$                             contact.php [QSA,L]

RewriteRule ^template-(.*).html$                       template.php?a=$1 [QSA,L]
RewriteRule ^website-(.*).html$                        website.php?a=$1 [QSA,L]
RewriteRule ^applets-(.*).html$                        applets.php?a=$1 [QSA,L]
RewriteRule ^system-(.*).html$                         system.php?a=$1 [QSA,L]
RewriteRule ^about_us.html$                            about_us.php [QSA,L]
RewriteRule ^contact-(.*).html$                        contact.php?a=$1 [QSA,L]